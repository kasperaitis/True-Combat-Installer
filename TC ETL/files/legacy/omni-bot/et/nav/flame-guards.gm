	Lever_used = function( trigger )
	{
		if ( Map.Phase_4 )
		{
			Util.MapDebugPrint( "^3Lever_used" );
			
			Map.Switches.outside_switch.Enabled = false;
			Map.Switches.inside_switch.Enabled = false;
			
			vel1 = ToVector(trigger.Action);
			
			if ( vel1[ 2 ] == -16 )
			{
				Map.Door_closing = false;
				Map.Door_opening = true;
				
				Util.MapDebugPrint( "^3Door_opening" );
			}
			else if ( vel1[ 2 ] == 16 )
			{
				Map.Door_opening = false;
				Map.Door_closing = true;
				
				Map.Door_opened = false;
				
				SetAvailableMapGoals( TEAM.ALLIES, false, {
					"SWITCH_Inside",
					"SWITCH_Outside",
				});
				
				Util.MapDebugPrint( "^3Door_closing" );
			}
		}
	},
	
	Door_open = function( trigger )
	{
		if ( Map.Phase_4 )
		{
			vel = ToVector(trigger.Action);
			
			if ( vel[ 0 ] == 15 )
			{
				sleep(2.5);
				
				if ( Map.Door_opening )
				{
					Map.Door_opened = true;
				}
				else
				{
					Map.Door_opened = false;
				}
				
				sleep(2);
				
				if ( Map.Door_opening )
				{
					if ( !Map.GeneratorDestroyed )
					{
						SetAvailableMapGoals( TEAM.ALLIES, true, {
							"SWITCH_Inside",
							"SWITCH_Outside",
						});
					}
					
					Util.MapDebugPrint( "^3Door_opened" );
				}
			}
			else if ( vel[ 0 ] == -15 )
			{
				sleep(3);
				
				if ( Map.Door_closing )
				{
					Map.Switches.outside_switch.Enabled = true;
					Map.Switches.inside_switch.Enabled = true;
					
					Util.MapDebugPrint( "^3Door_closed" );
				}
			}
		}
	},
	

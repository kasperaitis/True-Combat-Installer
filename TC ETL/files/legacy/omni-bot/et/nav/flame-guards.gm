		OnExit = function(ent)
		{
			if ( Map.Phase_2 )
			{
				if( GetEntTeam(ent) == TEAM.AXIS )
				{
					Map.Axis_InPhase2 -= 1;
					
					if ( Map.Axis_InPhase2 == 0 )
